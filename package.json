    "karma-chrome-launcher": "^1.0.1",
    "karma-cli": "^1.0.1",
    "karma-htmlfile-reporter": "^0.3.4",
    "karma-jasmine": "^1.0.2",
    "karma-spec-reporter": "0.0.26",
    "lite-server": "^2.2.2",
